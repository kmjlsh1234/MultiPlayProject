<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	
	<packet name ="C_Chat">
		<string name ="message"/>
	</packet>
  
  <packet name ="C_ReadyPacket">
    <bool name="isReady"/>
  </packet>
	
	<packet name ="S_BroadCast_ReadyPacket">
		<int name ="sessionId"/>
		<bool name ="isReady"/>
	</packet>
	
	<packet name ="C_StartPacket">
	</packet>

	<packet name ="S_BroadCast_StartPacket">
	</packet>


	<packet name ="C_MovePacket">
    <int name ="playerId"/>
    <float name ="posX"/>
    <float name ="posY"/>
    <float name="posZ"/>
    <float name ="rotY"/>
    <int name ="tick"/>
  </packet>

  <packet name ="S_BroadCast_MovePacket">
    <int name ="playerId"/>
    <float name ="posX"/>
    <float name ="posY"/>
    <float name="posZ"/>
    <float name ="rotY"/>
    <int name ="tick"/>
  </packet>
  
	<packet name ="S_BroadCast_Chat">
		<int name ="sessionId"/>
		<string name ="message"/>
	</packet>

	<packet name ="C_ExitRoom">
	</packet>

	<packet name ="S_BroadCast_ExitRoom">
		<int name ="sessionId"/>
	</packet>

  <packet name ="S_BroadCast_ChangeRoomInfo">
	  <int name ="roomId"/>
	  <string name ="roomName"/>
	  <int name ="masterId"/>
  </packet>
  
  <packet name ="C_CreateRoom">
    <string name ="roomName"/>
  </packet>

	<packet name ="C_CreateOrJoinRoom">
	</packet>
	
	<packet name = "C_EnterRoom">
    <int name ="roomId"/>
	</packet>
	
  <packet name ="C_RoomList">
  </packet>
  
	<packet name ="S_BroadCast_EnterRoom">
		<int name ="sessionId"/>
	</packet>

	<packet name ="S_RoomInfo">
		<int name ="roomId"/>
		<string name ="roomName"/>
		<int name ="masterId"/>
		<list name ="Player">
			<int name ="sessionId"/>
			<bool name ="isMaster"/>
			<bool name ="isSelf"/>
			<bool name ="isReady"/>
		</list>
	</packet>

  <packet name ="S_RoomList">
    <list name ="Room">
      <int name ="roomId"/>
      <string name ="roomName"/>
    </list>
  </packet>

	<packet name ="S_ErrorCode">
		<int name ="code"/>
		<string name ="message"/>
	</packet>
  
  
</PDL>