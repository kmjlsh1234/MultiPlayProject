<?xml version="1.0" encoding="utf-8" ?>
<PDL>
	
	<packet name ="C_Chat">
		<string name ="message"/>
	</packet>
  
	<packet name ="S_BroadCast_Chat">
		<int name ="sessionId"/>
		<string name ="message"/>
	</packet>

	<packet name ="C_ExitRoom">
	</packet>

	<packet name ="S_BroadCast_ExitRoom">
		<int name ="sessionId"/>
	</packet>

  <packet name ="C_CreateRoom">
    <string name ="roomName"/>
  </packet>
  
	<packet name = "C_EnterRoom">
    <int name ="roomId"/>
	</packet>
  
	<packet name ="S_BroadCast_EnterRoom">
		<int name ="sessionId"/>
	</packet>
	
	<packet name ="TestPacket">
		<int name = "playerId"/>
		<string name ="message"/>
	</packet>

	<packet name ="S_PlayerList">
		<list name ="Player">
			<int name ="sessionId"/>
			<bool name ="isSelf"/>
		</list>
	</packet>

  <packet name ="C_RoomList">
  </packet>
  
  <packet name ="S_RoomList">
    <list name ="Room">
      <int name ="roomId"/>
      <string name ="roomName"/>
    </list>
  </packet>

	<packet name ="S_ErrorCode">
		<int name ="code"/>
		<string name ="message"/>
	</packet>
  
  
</PDL>